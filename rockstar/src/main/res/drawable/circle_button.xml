<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">
            <solid android:color="#3f5e5e5e"/>
            <stroke android:width="3dp" android:color="#efa6a4a5"/>
        </shape>
    </item>
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">
            <solid android:color="#0f5e5e5e"/>
            <stroke android:width="2dp" android:color="#efffffff"/>
        </shape>
    </item>
</selector>